-include makefile_defs

CUSTINV.FILE : QDDSSRC.FILE/CUSTINV.MBR \
    $(ASPPREFIX)/QSYS.LIB/$(LIB1).LIB/CUSMST.FILE \
    $(ASPPREFIX)/QSYS.LIB/$(LIB1).LIB/INVMST.FILE
	system -v -k "EDOCRTOBJ BLDTGT($(LIB1)/CUSTINV) OBJTYPE(*FILE) OBJATTR(LF) CMD(CRTLF) CMDPARMS(' FILE($(LIB1)/CUSTINV) SRCFILE(QDDSSRC) SRCMBR(CUSTINV) TEXT(''Customer invoices'')')"

CUSMST.FILE : QDDSSRC.FILE/CUSMST.MBR
	system -v -k "EDOCRTOBJ BLDTGT($(LIB1)/CUSMST) OBJTYPE(*FILE) OBJATTR(PF) CMD(CRTPF) CMDPARMS(' FILE($(LIB1)/CUSMST) SRCFILE(QDDSSRC) SRCMBR(CUSMST) TEXT(''Customer Master'')')"

INVMST.FILE : QDDSSRC.FILE/INVMST.MBR
	system -v -k "EDOCRTOBJ BLDTGT($(LIB1)/INVMST) OBJTYPE(*FILE) OBJATTR(PF) CMD(CRTPF) CMDPARMS(' FILE($(LIB1)/INVMST) SRCFILE(QDDSSRC) SRCMBR(INVMST) TEXT(''Customer Master'')')"

